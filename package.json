{
  "name": "happn-3",
  "description": "pub/sub api as a service using primus and mongo & redis or nedb, can work as cluster, single process or embedded using nedb",
  "version": "7.2.2",
  "main": "./lib/index",
  "protocol": "2",
  "database": "1",
  "scripts": {
    "test": "gulp lint && mocha silence.js test/integration/** test/unit/**",
    "test-unit": "mocha silence.js test/unit/**",
    "test-cover": "istanbul cover _mocha -- silence.js test/integration/** test/unit/**",
    "test-browser": "gulp --gulpfile test/browser/gulp-01.js",
    "test-longrunning": "node test/test-longrunning/serial-run.js",
    "test-sanity": "mocha silence.js test/integration/**/*sanity.js",
    "test-release": "rm -rf node_modules && rm -f package-lock.json && npm install && npm test",
    "test-010": "source ~/.nvm/nvm.sh && nvm use 0.10 && rm -rf node_modules && npm install && node_modules/mocha/bin/_mocha test/integration/**",
    "test-lint": "gulp lint"
  },
  "author": "Tenacious and NSOFT",
  "license": "MIT",
  "keywords": [
    "mongo",
    "redis",
    "nedb",
    "primus",
    "websockets"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/happner/happn-3.git"
  },
  "dependencies": {
    "async": "1.5.2",
    "bluebird": "2.10.2",
    "body-parser": "1.14.1",
    "connect": "3.4.0",
