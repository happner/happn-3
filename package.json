    "handlebars": "4.0.11",
    "happn-logger": "0.0.2",
    "happn-nedb": "1.8.1",
    "happn-password-hash-and-salt": "1.1.0",
    "happn-primus": "5.2.4",
    "happn-sillyname": "^0.1.1",
    "happn-stats": "1.0.6",
    "happn-tcp-port-used": "0.1.3",
    "happn-util-crypto": "0.2.3",
    "happner-hyperid": "2.0.0",
    "homedir": "0.6.0",
    "jsonpack": "1.1.5",
